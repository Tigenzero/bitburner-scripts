disableLog('getServerSecurityLevel')
disableLog('getServerMoneyAvailable')
MAX_SEC = 8
MIN_MON = 2000000
servers = args[0].split(',')
while(true) {
	for (i in servers){
		server = servers[i];
		if (getServerSecurityLevel(server) <= MAX_SEC && 
		getServerMoneyAvailable(server) >= MIN_MON){
			hack(server);
		}
	}
}